<%= form_with url: path, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
   
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.number_field :zip %>

  <%= f.submit "Submit" %>
<% end %>

